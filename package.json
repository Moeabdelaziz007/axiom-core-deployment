{
    "name": "axiom-core-deployment",
    "version": "1.0.0",
    "packageManager": "npm@10.8.2",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "migrate": "npx ts-node scripts/migrate.ts",
        "test:db": "npx ts-node scripts/test-foundation.ts",
        "test:api": "npx ts-node scripts/test-api.ts",
        "setup:phase1": "npm run migrate && npm run test:db",
        "validate:env": "npx ts-node scripts/validate-env.ts",
        "deploy:prod": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh",
        "deploy:staking": "cd src/infra/solana/programs/axiom-staking && anchor build && npx ts-node scripts/deploy-staking-mainnet.ts",
        "test:staking": "cd src/infra/solana/programs/axiom-staking && anchor test",
        "test:prod": "NODE_ENV=production npm run test:staking",
        "test:e2e": "npx ts-node scripts/test-staking-e2e.ts",
        "test": "npx ts-node scripts/run-comprehensive-tests.ts",
        "test:unit": "TEST_TYPE=unit npx ts-node scripts/run-comprehensive-tests.ts",
        "test:integration": "TEST_TYPE=integration npx ts-node scripts/run-comprehensive-tests.ts",
        "test:performance": "TEST_TYPE=performance npx ts-node scripts/run-comprehensive-tests.ts",
        "test:security": "TEST_TYPE=security npx ts-node scripts/run-comprehensive-tests.ts",
        "test:validation": "TEST_TYPE=validation npx ts-node scripts/run-comprehensive-tests.ts",
        "test:webhook": "npx jest --config jest.config.webhook.js",
        "test:ci": "FAIL_FAST=true GENERATE_REPORTS=true npx ts-node scripts/run-comprehensive-tests.ts",
        "test:env:setup": "npx ts-node scripts/setup-test-env.ts setup",
        "test:env:cleanup": "npx ts-node scripts/setup-test-env.ts cleanup",
        "test:env:validate": "npx ts-node scripts/setup-test-env.ts validate",
        "audit:security": "npx ts-node scripts/security-audit.ts",
        "audit:comprehensive": "TEST_TYPE=security npx ts-node scripts/run-comprehensive-tests.ts",
        "quality:check": "TEST_TYPE=all GENERATE_REPORTS=true npx ts-node scripts/run-comprehensive-tests.ts",
        "monitor:prod": "NODE_ENV=production npm run start",
        "monitor:tests": "npx ts-node scripts/monitor-test-performance.ts",
        "rollback:create": "npx ts-node scripts/rollback-procedures.ts create",
        "rollback:execute": "npx ts-node scripts/rollback-procedures.ts execute",
        "rollback:emergency": "npx ts-node scripts/rollback-procedures.ts emergency",
        "rollback:list": "npx ts-node scripts/rollback-procedures.ts list",
        "backup:create": "npx ts-node scripts/rollback-procedures.ts backup",
        "validate:pre-deploy": "TEST_TYPE=validation SECURITY_SCANNING=true npx ts-node scripts/run-comprehensive-tests.ts",
        "validate:agent": "npx ts-node scripts/validate-agent-deployment.ts",
        "coverage:generate": "GENERATE_COVERAGE=true npx ts-node scripts/run-comprehensive-tests.ts",
        "coverage:report": "npx ts-node scripts/generate-coverage-report.ts",
        "report:quality": "npx ts-node scripts/generate-quality-report.ts"
    },
    "dependencies": {
        "@ai-sdk/google": "^2.0.44",
        "@ai-sdk/openai": "^2.0.73",
        "@ai-sdk/react": "^2.0.103",
        "@coral-xyz/anchor": "^0.32.1",
        "@glasseaters/hydra-sdk": "^0.3.2",
        "@google/generative-ai": "^0.24.1",
        "@langchain/cloudflare": "^1.0.1",
        "@langchain/core": "^1.1.0",
        "@langchain/google-genai": "^2.0.0",
        "@langchain/langgraph": "^1.0.2",
        "@libsql/client": "^0.15.15",
        "@qdrant/js-client-grpc": "^1.9.0",
        "@qdrant/js-client-rest": "^1.9.0",
        "@radix-ui/react-avatar": "^1.1.11",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-navigation-menu": "^1.2.14",
        "@radix-ui/react-progress": "^1.1.8",
        "@radix-ui/react-scroll-area": "^1.2.10",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-separator": "^1.1.8",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-tabs": "^1.1.13",
        "@solana/spl-token": "^0.4.14",
        "@solana/web3.js": "^1.98.4",
        "@supabase/supabase-js": "^2.39.0",
        "@tanstack/react-query": "^5.90.11",
        "@types/d3": "^7.4.3",
        "@upstash/ratelimit": "^2.0.7",
        "@upstash/redis": "^1.35.7",
        "ai": "^5.0.104",
        "bs58": "^6.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "d3": "^7.9.0",
        "dotenv": "^17.2.3",
        "framer-motion": "^12.23.25",
        "helius-sdk": "^2.0.3",
        "langchain": "^1.1.1",
        "lucide-react": "^0.554.0",
        "next": "^16.0.3",
        "puppeteer": "^24.31.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "solana-agent-kit": "^1.1.0",
        "tailwind-merge": "^3.4.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@mswjs/data": "^0.16.0",
        "@types/artillery": "^1.7.5",
        "@types/chai": "^5.2.3",
        "@types/jest": "^29.5.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/sinon": "^17.0.0",
        "@types/sqlite3": "^3.1.0",
        "@types/supertest": "^2.0.16",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitest/coverage-v8": "^1.0.0",
        "@vitest/ui": "^1.0.0",
        "allure-js-commons": "^2.6.0",
        "allure-vitest": "^2.0.0",
        "artillery": "^2.0.0",
        "autoprefixer": "^10.4.17",
        "chai": "^4.5.0",
        "eslint": "^8.55.0",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "jest-axe": "^10.0.0",
        "jest-environment-jsdom": "^30.2.0",
        "jest-environment-node": "^29.7.0",
        "jest-html-reporters": "^3.1.0",
        "jest-junit": "^16.0.0",
        "lint-staged": "^15.2.0",
        "miniflare": "^3.0.0",
        "msw": "^2.0.0",
        "postcss": "^8.4.35",
        "prettier": "^3.1.0",
        "sinon": "^17.0.0",
        "sqlite3": "^5.1.0",
        "supertest": "^6.3.0",
        "tailwindcss": "^3.4.1",
        "tailwindcss-animate": "^1.0.7",
        "ts-jest": "^29.1.0",
        "ts-mocha": "^10.0.0",
        "typescript": "^5",
        "uuid": "^10.0.0",
        "vitest": "^1.0.0"
    }
}
